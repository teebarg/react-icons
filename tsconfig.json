{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": true,
        "module": "es2015",
        "target": "es5",
        "lib": [
            "es2017",
            "dom"
        ],
        "jsx": "react",
        "moduleResolution": "node",
        "baseUrl": ".",
        "declaration": true,
        "strict": true,
        "noImplicitAny": true,
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}
